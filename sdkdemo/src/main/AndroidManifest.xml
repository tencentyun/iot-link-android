    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".core.activity.BindMobilePhoneActivity" />
        <activity android:name=".core.activity.ForgotPasswordActivity" />
        <activity android:name=".core.activity.SetPasswordActivity" />
        <activity android:name=".core.activity.ShareDeviceListActivity" />
        <activity android:name=".core.activity.ShareDeviceActivity" />
        <activity android:name=".core.activity.ShareUserListActivity" />
        <activity android:name=".core.activity.SelectRoomActivity" />
        <activity android:name=".core.activity.WeekRepeatActivity" />
        <activity android:name=".core.activity.AddTimingProjectActivity" />
        <activity android:name=".core.activity.TimingProjectActivity" />
        <activity android:name=".core.activity.ConnectDeviceActivity" />
        <activity android:name=".core.activity.ScanBindActivity" />
        <activity android:name=".core.activity.AddRoomActivity" />
        <activity android:name=".core.activity.RoomActivity" />
        <activity android:name=".core.activity.MemberActivity" />
        <activity android:name=".core.activity.InviteMemberActivity" />
        <activity android:name=".core.activity.RoomListActivity" />
        <activity android:name=".core.activity.FamilyActivity" />
        <activity android:name=".core.activity.AddFamilyActivity" />
        <activity android:name=".core.activity.FeedbackActivity" />
        <activity android:name=".core.activity.MessageActivity" />
        <activity android:name=".core.activity.PersonalInfoActivity" />
        <activity android:name=".core.activity.FamilyListActivity" />
        <activity android:name=".core.activity.ControlPanelActivity" />
        <activity android:name=".core.activity.RegisterActivity" />
        <activity android:name=".core.activity.ConfigNetActivity" />
        <activity android:name=".core.activity.QrcodeConfigNetActivity" />
        <activity android:name=".core.activity.ApConfigNetActivity" />
        <activity android:name=".core.activity.WiredConfigNetActivity" />
        <activity android:name=".video.activity.VideoOptionsActivity" />
        <activity android:name=".video.activity.VideoInputAuthorizeActivity" />
        <activity android:name=".video.activity.VideoMainActivity" />
        <activity android:name=".video.activity.VideoNvrActivity" />
        <activity android:name=".video.activity.VideoPlaybackActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"/>
        <activity android:name=".video.activity.VideoMultiPreviewActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"/>
        <activity android:name=".video.activity.VideoPreviewActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"/>
        <activity android:name="com.tencent.iot.explorer.link.rtc.ui.audiocall.TRTCAudioCallActivity" />
        <activity android:name="com.tencent.iot.explorer.link.rtc.ui.videocall.TRTCVideoCallActivity" />
        <activity android:name=".core.activity.LoginActivity" />
        <activity android:name=".ModuleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".core.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity android:name=".core.activity.AddDeviceActivity" />
    </application>

</manifest>